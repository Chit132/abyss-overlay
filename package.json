{
  "name": "abyss-overlay",
  "productName": "Abyss Overlay",
  "version": "2.0.0",
  "description": "The Official Abyss Stats Overlay",
  "homepage": "https://github.com/Chit132/abyss-overlay",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-installer": "electron-builder --win --x64 --ia32",
    "build-installer-dev": "electron-builder --win --x64",
    "build-mac": "electron-builder --mac",
    "linux": "electron-packager . Abyss-Overlay --platform=linux",
    "publish": "electron-builder --win --x64 --ia32 --publish always"
  },
  "build": {
    "appId": "abyss-overlay",
    "artifactName": "${productName} Setup v${version} ${arch}.${ext}",
    "asarUnpack": [
      "exec"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "publish": [
        "github"
      ],
      "icon": "assets/logo.ico"
    },
    "nsis": {
      "installerIcon": "assets/logo.ico",
      "uninstallerIcon": "assets/logo.ico",
      "uninstallDisplayName": "Abyss Overlay",
      "license": "LICENSE.txt",
      "oneClick": false
    },
    "mac": {
      "artifactName": "${productName} Setup v${version}_MAC.${ext}",
      "category": "public.app-category.games",
      "target": [
        "dmg"
      ],
      "icon": "assets/logo.png"
    },
    "dmg": {
      "backgroundColor": "#C100AA",
      "icon": "assets/logo.png"
    }
  },
  "keywords": [],
  "author": "Chit",
  "license": "GPL-3.0-or-later",
  "email": "",
  "repository": "github:Chit132/abyss-overlay",
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "@simonwep/pickr": "^1.8.0",
    "boolean": "^3.0.2",
    "buffer-crc32": "^0.2.13",
    "buffer-from": "^1.1.1",
    "cacheable-request": "^6.1.0",
    "clone-response": "^1.0.2",
    "concat-stream": "^1.6.2",
    "config-chain": "^1.1.12",
    "core-js": "^3.9.1",
    "core-util-is": "^1.0.2",
    "debug": "^4.3.1",
    "decompress-response": "^3.3.0",
    "defer-to-connect": "^1.1.3",
    "define-properties": "^1.1.3",
    "detect-node": "^2.0.4",
    "discord-rpc": "^4.0.1",
    "dotenv": "^16.0.1",
    "duplexer3": "^0.1.4",
    "electron-is-dev": "^2.0.0",
    "electron-json-config": "^1.5.3",
    "electron-log": "^4.4.7",
    "electron-updater": "^5.0.1",
    "encodeurl": "^1.0.2",
    "end-of-stream": "^1.4.4",
    "env-paths": "^2.2.0",
    "es6-error": "^4.1.1",
    "escape-string-regexp": "^4.0.0",
    "extract-zip": "^1.7.0",
    "fd-slicer": "^1.1.0",
    "fs-extra": "^8.1.0",
    "get-stream": "^4.1.0",
    "global-agent": "^2.1.12",
    "global-tunnel-ng": "^2.7.1",
    "globalthis": "^1.0.2",
    "got": "^9.6.0",
    "graceful-fs": "^4.2.6",
    "http-cache-semantics": "^4.1.0",
    "inherits": "^2.0.4",
    "ini": "^1.3.8",
    "isarray": "^1.0.0",
    "jquery": "^3.6.0",
    "json-buffer": "^3.0.0",
    "json-stringify-safe": "^5.0.1",
    "jsonfile": "^4.0.0",
    "keyv": "^3.1.0",
    "lodash": "^4.17.21",
    "lowercase-keys": "^1.0.1",
    "lru-cache": "^6.0.0",
    "matcher": "^3.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "mimic-response": "^1.0.1",
    "minimist": "1.2.6",
    "mkdirp": "^0.5.5",
    "ms": "^2.1.2",
    "normalize-url": "^4.5.0",
    "npm-conf": "^1.1.3",
    "object-keys": "^1.1.1",
    "once": "^1.4.0",
    "p-cancelable": "^1.1.0",
